{
  "compilerOptions": {
      "lib": ["dom", "es5", "es2015"],
      "outDir": "./dist/electron/",
      "sourceMap": true,
      "strict": true,
      "module": "es2015",
      "moduleResolution": "node",
      "target": "es5",
      "baseUrl": "./",
      "paths": {
        "@": ["src/renderer"],
        "vue$": ["node-modules/vue/dist/vue.esm.js"]
      },
      "allowJs": true,
      "typeRoots": ["typings"],
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true
  }
}
